<Window x:Class="DiplomaProject.WaitDialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Wait" SizeToContent="WidthAndHeight">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ProgressBar Height="20" Margin="0" IsIndeterminate="True"/>
        <TextBlock Grid.Row="1" Name="ipInterfacesTextBlock"/>
    </Grid>
</Window>
